#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n_length, num;
    vector<int> v;
    int q, query;
    cin >> n_length;

    // Make a vector of N numbers
    for(int m=0;m<n_length;m++)
    {
        cin >> num;
        v.push_back(num);
    }
    
    // Get num of queries
    cin >> q;
    vector<int>::iterator low;
    
    // Loop for query entries
    for(int i= 0; i < q;i++)
    {
        cin >> query;
        
        // Grab index
        low = lower_bound(v.begin(), v.end(), query);
        
        // Use if statement for condition 1
        if (low != v.end() && *low == query)
        {
            cout << "Yes " << low-v.begin() + 1 << endl;
        }
        // Use else statement for condition 2
        else {
        cout << "No " << low-v.begin()+1 << endl;
        }
    }
            
    return 0;
}
